{
  "name": "git-repo-npm-bootster",
  "version": "1.0.2",
  "description": "A cli to that automatically generate all the needed files to make your repo or npm package look good and reach the 100 quality score ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node bin/index.js",
    "delete": "node src/utils/deleteAllFiles.js"
  },
  "bin": {
    "boost-git-repo": "bin/index.js"
  },
  "_moduleAliases": {
    "@root": ".",
    "@utils": "src/utils",
    "@buildFiles": "src/buildFiles",
    "@questions": "src/questions"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YIZHUANG/git-repo-npm-bootster"
  },
  "bugs": {
    "url": "https://github.com/YIZHUANG/git-repo-npm-bootster/issues"
  },
  "author": "Yi Zhuang",
  "license": "MIT",
  "dependencies": {
    "auto-changelog": "^1.13.0",
    "date-fns": "^1.30.1",
    "ejs": "^2.6.2",
    "generate-contributing": "git+https://github.com/YIZHUANG/generate-contributing.git",
    "git-repo-name": "^1.0.1",
    "inquirer": "^6.3.1",
    "load-json-file": "^6.1.0",
    "lodash": "^4.17.11",
    "module-alias": "^2.2.0",
    "ora": "^3.4.0",
    "prompts": "^2.1.0",
    "readme-md-generator": "^0.3.0"
  },
  "keywords": [
    "Nodejs",
    "Cli tool",
    "Npm package quality",
    "Ranking",
    "Git repo",
    "README.md",
    "contributing.md",
    "JavaScript"
  ],
  "homepage": "https://github.com/YIZHUANG/git-repo-npm-bootster#readme"
}
